    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.5.0",
    "leasot": "^6.3.0",
    "nyc": "^12.0.2",
    "pixtest": "0.0.1",
    "prettier": "^1.13.6",
    "sonar-scanner": "^3.1.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.18.0",
    "dist-exiftool": "^10.53.0",
    "fs-extra": "^7.0.0",
    "jsonfile": "^4.0.0",
    "lodash.foreach": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.keyby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
